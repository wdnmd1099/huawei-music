ine":188,"column":22},"generated":{"line":126,"column":22}},{"source":"src\\scss\\index.scss","name":null,"original":{"line":189,"column":10},"generated":{"line":127,"column":10}},{"source":"src\\scss\\index.scss","name":null,"original":{"line":189,"column":60},"generated":{"line":127,"column":78}},{"source":"src\\scss\\index.scss","name":null,"original":{"line":190,"column":10},"generated":{"line":128,"column":10}},{"source":"src\\scss\\index.scss","name":null,"original":{"line":190,"column":34},"generated":{"line":128,"column":34}},{"source":"src\\scss\\index.scss","name":null,"original":{"line":190,"column":34},"generated":{"line":128,"column":36}},{"source":"src\\scss\\index.scss","name":null,"original":{"line":197,"column":0},"generated":{"line":130,"column":0}},{"source":"src\\scss\\index.scss","name":null,"original":{"line":199,"column":2},"generated":{"line":131,"column":2}},{"source":"src\\scss\\index.scss","name":null,"original":{"line":200,"column":4},"generated":{"line":132,"column":4}},{"source":"src\\scss\\index.scss","name":null,"original":{"line":200,"column":24},"generated":{"line":132,"column":24}},{"source":"src\\scss\\index.scss","name":null,"original":{"line":200,"column":24},"generated":{"line":132,"column":26}},{"source":"src\\scss\\index.scss","name":null,"original":{"line":202,"column":2},"generated":{"line":133,"column":2}},{"source":"src\\scss\\index.scss","name":null,"original":{"line":203,"column":4},"generated":{"line":134,"column":4}},{"source":"src\\scss\\index.scss","name":null,"original":{"line":203,"column":29},"generated":{"line":134,"column":29}},{"source":"src\\scss\\index.scss","name":null,"original":{"line":203,"column":29},"generated":{"line":134,"column":31}},{"source":"src\\scss\\index.scss","name":null,"original":{"line":203,"column":29},"generated":{"line":134,"column":33}}],"sources":{"src\\scss\\index.scss":"$backgroundColor: #060a3d;\n$color: #fff;\n$color1: #868aaf;\n$color2: #db3baa;\n$color2-dark: darken($color2, 10%);\n$color2-darker: darken($color2, 30%);\n$color3: #0025f1;\n\n* {\n  margin: 0;\n  padding: 0;\n}\n\n#player {\n  height: 100vh;\n  background: $backgroundColor;\n  .header {\n    height: 100px;\n    text-align: center;\n    h1 {\n      color: $color;\n      font-size: 20px;\n      padding-top: 20px;\n    }\n    p {\n      color: $color1;\n      font-size: 12px;\n    }\n    .balls {\n      display: flex; //居中万金油 1\n      justify-content: center; //居中万金油2  水平居中\n      align-items: center; //垂直居中\n      margin-top: 20px;\n      span {\n        display: block;\n        width: 5px;\n        height: 5px;\n        background: $color2-darker;\n        border-radius: 50%;\n        margin: 0 4px;\n\n        &.current {\n          width: 8px;\n          height: 8px;\n          background: $color2;\n        }\n      }\n    }\n  }\n}\n\n.page1-effects {\n    // width: 100vw;\n    height: calc(100vh - 280px);\n    // display: flex;\n    // align-items: center;\n\n    // transition: transform 0.3s;\n\n    // &.panel1 {\n    //   transform: translateX(0vw);\n    // }\n    // &.panel2 {\n    //   transform: translateX(-100vw);\n    // }\n  .panel-effect {\n    // width: 100vw;\n    // height: 100%;\n    // display: flex;\n    // flex-direction: column;\n    // justify-content: center;\n    .effect {\n      position: relative;\n      display: flex;\n      justify-content: center;\n      align-items: center;\n      height: 40vh;\n      > div {\n        background-size: contain; //https://blog.csdn.net/caicai1171523597/article/details/105876002\n        position: absolute;\n      }\n\n      .effect-1 {\n        background: url(\"../svg/effect-no-move.svg\") 0 0 no-repeat; //https://blog.csdn.net/x1037490413/article/details/102922939\n        width: 70vw;\n        height: 70vw;\n        animation: rotate1 20s linear infinite; //旋转特效调用  rotate1调用下面写好的旋转设置， 10s完成一次旋转 linear 线性的  infinite无穷的，一直转\n      }\n      .effect-2 {\n        background: url(\"../svg/effect-move1.svg\") 0 0 no-repeat;\n        width: 60vw;\n        height: 60vw;\n        animation: rotate1 10s linear infinite reverse; //reverse 反过来转\n      }\n      .effect-3 {\n        background: url(\"../svg/effect-move2.svg\") 0 0 no-repeat;\n        width: 24vw;\n        height: 24vw;\n        animation: rotate1 10s linear infinite;\n      }\n    }\n\n    .lyric {\n      text-align: center;\n      p {\n        font-size: 13px;\n        color: $color1;\n        margin-top: 10px;\n\n        &.current-lyric {\n          //表示既是p标签 也是class为current 相当于是指定p标签\n          color: $color;\n        }\n      }\n    }\n  }\n}\n\n.footer {\n  height: 180px;\n  .buttons {\n    display: flex;\n    justify-content: center;\n    align-items: center;\n\n    svg {\n      width: 24px;\n      height: 24px;\n      margin: 0 20px;\n      margin-top: 20px;\n    }\n  }\n\n  .actions {\n    display: flex;\n    align-items: center;\n    justify-content: space-between;\n    padding: 20px 20px;\n\n    svg {\n      width: 28px;\n      height: 28px;\n\n      &.btn-play-pause {\n        width: 50px;\n        height: 50px;\n      }\n    }\n  }\n\n  .bar-area {\n    color: $color1;\n    font-size: 12px;\n    display: flex;\n    padding: 0 20px;\n    margin-top: 20px;\n    align-items: center;\n    .time-start {\n      order: 1;\n      width: 40px;\n    }\n    .time-end {\n      order: 3;\n      width: 40px;\n    }\n    .bar {\n      order: 2;\n      flex: 1;\n      height: 4px;\n      background: $color3;\n      border-radius: 2px;\n      margin: 0 10px;\n      .progress {\n        width: 0%;\n        transition: all 0.2s;\n        height: 100%;\n        border-radius: 2px;\n        background: $color2;\n        position: relative;\n\n        &::after {\n          content: \"\";\n          position: absolute;\n          right: -8px;\n          top: -7px;\n          display: block;\n          width: 16px;\n          height: 16px;\n          background: url(../svg/progress.svg) 0 0 no-repeat;\n          background-size: contain;\n        }\n      }\n    }\n  }\n}\n\n@keyframes rotate1 {\n  //旋转特效\n  0% {\n    transform: rotate(0);\n  }\n  100% {\n    transform: rotate(360deg);\n  }\n}\n"},"lineCount":null}},"error":null,"hash":"98e1e829dbf211c8327157b455e944db","cacheData":{"env":{}}}