ign-items: center;\r\n        height: 60vh;\r\n        >div{\r\n            background-size: contain; //https://blog.csdn.net/caicai1171523597/article/details/105876002\r\n            position: absolute;\r\n        }\r\n\r\n        .effect-1{\r\n            background: url(\"../svg/effect-no-move.svg\") 0 0 no-repeat;  //https://blog.csdn.net/x1037490413/article/details/102922939\r\n            width: 70vw;\r\n            height: 70vw;\r\n            animation: rotate1 20s linear infinite; //旋转特效调用  rotate1调用下面写好的旋转设置， 10s完成一次旋转 linear 线性的  infinite无穷的，一直转\r\n        }\r\n        .effect-2{\r\n            background: url(\"../svg/effect-move1.svg\") 0 0 no-repeat;\r\n            width: 60vw;\r\n            height: 60vw;\r\n            animation: rotate1 10s linear infinite reverse ;  //reverse 反过来转\r\n        }\r\n        .effect-3{\r\n            background: url(\"../svg/effect-move2.svg\") 0 0 no-repeat;\r\n            width: 24vw;\r\n            height: 24vw;\r\n            animation: rotate1 10s linear infinite; \r\n        }\r\n    }\r\n    .balls{\r\n        display: flex;       //居中万金油 1\r\n        justify-content: center; //居中万金油2  水平居中\r\n        align-items:center;//垂直居中\r\n        margin-top: 20px;\r\n        span{\r\n            display: block;\r\n            width: 5px;\r\n            height: 5px;\r\n            background:$color2-darker;\r\n            border-radius: 50%;\r\n            margin: 0 4px;\r\n\r\n            &.current{\r\n                width: 8px;\r\n                height: 8px;\r\n                background: $color2;\r\n            }\r\n        }\r\n    }\r\n    .lyric{\r\n        text-align: center;\r\n        p{\r\n            font-size: 13px;\r\n            color: $color1;\r\n            margin-top: 10px;\r\n            \r\n            &.current{  //表示既是p标签 也是class为current 相当于是指定p标签\r\n                color:$color;\r\n\r\n            }\r\n        }\r\n    }\r\n}\r\n\r\n.buttons{\r\n     display: flex;\r\n     justify-content: center;\r\n     align-items: center;\r\n    \r\n\r\n    svg{\r\n        width: 24px;\r\n        height: 24px;\r\n        margin:0 20px;\r\n        margin-top: 20px;\r\n    }\r\n}\r\n\r\n\r\n\r\n.bar-area {\r\n    color: $color1;\r\n    font-size: 12px;\r\n    display: flex;\r\n    padding: 0 20px;\r\n    margin-top: 20px;\r\n    align-items: center;\r\n    .time-start {\r\n      order: 1;\r\n      width: 40px;\r\n    }\r\n    .time-end {\r\n      order: 3;\r\n      width: 40px;\r\n    }\r\n    .bar {\r\n      order: 2;\r\n      flex: 1;\r\n      height: 4px;\r\n      background: $color3;\r\n      border-radius: 2px;\r\n      margin: 0 10px;\r\n      .progress {\r\n        width: 0%;\r\n        transition: all .2s;\r\n        height: 100%;\r\n        border-radius: 2px;\r\n        background: $color2;\r\n        position: relative;\r\n\r\n        &::after {\r\n          content: '';\r\n          position: absolute;\r\n          right: -8px;\r\n          top: -7px;\r\n          display: block;\r\n          width: 16px;\r\n          height: 16px;\r\n          background: url(../svg/progress.svg) 0 0 no-repeat;\r\n          background-size: contain;\r\n        }\r\n      }\r\n    }\r\n  }\r\n\r\n\r\n\r\n@keyframes rotate1{  //旋转特效\r\n    0%{\r\n        transform: rotate(0);\r\n    }\r\n    100%{\r\n        transform: rotate(360deg);\r\n    }\r\n}"},"lineCount":null}},"error":null,"hash":"d45ec73d06ce77c65ba77afd9333ab2a","cacheData":{"env":{}}}